{
  "version": 2,
  "builds": [
    {
      "src": "todo.html",
      "use": "@vercel/static"
    },
    {
      "src": "todo.css",
      "use": "@vercel/static"
    },
    {
      "src": "todo.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/todo.html"
    }
  ]
}
